{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
      "outDir": "dist",
      "declaration": true,
      "composite": true
    },
    "include": ["src"]
  }
  